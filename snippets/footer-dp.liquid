            {% if template == 'index' %}
            $( ".footer-content li.toggle-sub-nav a" ).click(function(e) {
              e.preventDefault();
              if($(window).width() >= 767){
                if($('li.toggle-sub-nav a').hasClass('in-active')){
                }else{
                  var footer_h = $('.site-footer').innerHeight();
                  var id_h = $(this).attr('href');

                  var store_h = $(id_h).innerHeight() - 110;
                  var leftVal = $('.template-index #fullpage').get(0).style.transform;

                  var val_0 = (parseInt(leftVal.split(",")[1].replace('px','')) - store_h);
                  var val_1 = (parseInt(leftVal.split(",")[1].replace('px','')));
                  $('.template-index #fullpage').css({'transform':'translate3d(0, '+val_1+'px, 0)'});
                  setTimeout(function(){                
                    $('.template-index #fullpage').css({'transform':'translate3d(0, '+val_0+'px, 0)'});
                  }, 300);
                }
                $(this).addClass('in-active');
              }
              let urlID = '#' + getLastPartOfUrl(this.href);
              $('.footer-collapse > div').hide();
              $(urlID).toggle();             
            });
            {% else %}
            $( ".footer-content li.toggle-sub-nav a" ).click(function(e) {
              e.preventDefault();
              let urlID = '#' + getLastPartOfUrl(this.href);
