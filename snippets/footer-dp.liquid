<script>
  $('footer .signup-link').on('click', function(){
    $('#newsmodal.modal').css({'display':'block'});
  });
  $('.modal-content .close').on('click', function(){
    $('#newsmodal.modal').css({'display':'none'});
    $('.modal-backdrop').removeClass('in');
    $('#myModal').hide();
    $('body').removeClass('modal-open');
  });  
  $(document).on('keydown', function(event) {
       if (event.key == "Escape") {
           $('body').removeClass('modal-open');
       }
   });
  $('.dp-new-collapse').click(function(){ 
    $(this).children().text(function(i,old){
      return old=='+' ?  '-' : '+';
    });
    //$(this).parents('.dp-information').find('.collapse').toggle();
  });
  $('.sub > .dp-new-collapse').click(function(){ 
    $('.dp-information').toggleClass('footer-grey');
    $('.sub > .dp-new-collapse').toggleClass('footer-grey');
    $('.sub > div > a').toggleClass('footer-grey');
    $(this).toggleClass("footer-black");
    var href = $(this).attr('href');
    $(href).toggleClass("footer-black");
  });
  if($(window).width() <= 767){
    $('#home-footer').html('');
  }
</script>
