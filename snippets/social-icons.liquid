      {% if settings.social_youtube_url != blank %}
        <li><a aria-label="Youtube" class="youtube" target="_blank" rel="noopener" href="{{ settings.social_youtube_url }}">
        
          <svg xmlns="http://www.w3.org/2000/svg" width="25.436" height="25.436" viewBox="0 0 25.436 25.436">
