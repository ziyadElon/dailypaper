@mixin transformTXY($X,$Y){ transform:translate($X,$Y);-webkit-transform:translate($X,$Y);-moz-transform:translate($X,$Y);-o-transform:translate($X,$Y);-ms-transform:translate($X,$Y);}
@mixin transformTX($X){ transform:translateX($X);-webkit-transform:translateX($X);-moz-transform:translateX($X);-o-transform:translateX($X);-ms-transform:translateX($X);}
@mixin transformTY($Y){ transform:translateY($Y);-webkit-transform:translateY($Y);-moz-transform:translateY($Y);-o-transform:translateY($Y);-ms-transform:translateY($Y);}

@mixin transformOrigin($X,$Y){ transform-origin: $X $Y;-webkit-transform-origin: $X $Y;-moz-transform-origin: $X $Y;-o-transform-origin: $X $Y;-ms-transform-origin: $X $Y;}

@mixin transformScaleTY($X,$Y){ transform: scale($X) translateY($Y);-webkit-transform: scale($X) translateY($Y); -moz-transform: scale($X) translateY($Y);-o-transform: scale($X) translateY($Y);
  -ms-transform: scale($X) translateY($Y);}

@mixin transformRotate($val){ transform: rotate($val);-webkit-transform: rotate($val);-moz-transform: rotate($val);-o-transform: rotate($val);-ms-transform: rotate($val);}
@mixin transformRotateY($val){ transform: rotateY($val);-webkit-transform: rotateY($val);-moz-transform: rotateY($val);-o-transform: rotateY($val);-ms-transform: rotateY($val);}
@mixin transformRotateX($val){ transform: rotateX($val);-webkit-transform: rotateX($val);-moz-transform: rotateX($val);-o-transform: rotateX($val);-ms-transform: rotateX($val);}

@mixin transformSkew($val){ transform: skew($val);-webkit-transform: skew($val);-moz-transform: skew($val);-o-transform: skew($val);-ms-transform: skew($val);}
@mixin transformSkewX($val){ transform: skewX($val);-webkit-transform: skewX($val);-moz-transform: skewX($val);-o-transform: skewX($val);-ms-transform: skewX($val);}
@mixin transformSkewY($val){ transform: skewY($val);-webkit-transform: skewY($val);-moz-transform: skewY($val);-o-transform: skewY($val);-ms-transform: skewY($val);}

@mixin transform3DTXYZ($X,$Y,$Z){transform: translate3d($X,$Y,$Z);-webkit-transform: translate3d($X,$Y,$Z);-moz-transform: translate3d($X,$Y,$Z);-o-transform: translate3d($X,$Y,$Z);-ms-transform: translate3d($X,$Y,$Z);} 


@mixin transformScale($val){ transform: scale($val);-webkit-transform: scale($val);-moz-transform: scale($val);-o-transform: scale($val);-ms-transform: scale($val);}

@mixin transitionTime($val){ transition: all $val ease-in-out;-webkit-transition: all $val ease-in-out; -moz-transition: all $val ease-in-out; -o-transition: all $val ease-in-out; -ms-transform: all $val ease-in-out;}

@mixin transitionDelayTime($val){transition-delay: $val;-webkit-transition-delay: $val;-moz-transition-delay: $val;-o-transition-delay: $val;-ms-transition-delay: $val;}

@mixin boxShadow($X,$Y,$Blur,$Spred,$Color){box-shadow: $X $Y $Blur $Spred $Color;	-webkit-box-shadow: $X $Y $Blur $Spred $Color;-moz-box-shadow: $X $Y $Blur $Spred $Color;}

@mixin borderRadius($val){border-radius: $val;-webkit-border-radius: $val;-moz-border-radius: $val;}

@mixin brightness($X,$Y){-webkit-filter: brightness($X) invert($Y); filter: brightness($X) invert($Y);}
@mixin grayscale($X,$Y){  filter: $X; filter: grayscale($Y); -webkit-filter: grayscale($Y); }

@mixin opacity($X,$Y){ -ms-filter: "progid:DXImageTransform.Microsoft.Alpha(Opacity=$X)"; filter: alpha(opacity=$X); opacity: $Y; -moz-opacity: $Y; -khtml-opacity: $Y; }

@mixin widthHeight($X,$Y){ width:$X; height:$Y; }

@mixin placeholder {
  ::-webkit-input-placeholder {@content}
  :-moz-placeholder           {@content}
  ::-moz-placeholder          {@content}
  :-ms-input-placeholder      {@content}  
}

@mixin calc($property, $expression) {
  #{$property}: -moz-calc(#{$expression});
#{$property}: -webkit-calc(#{$expression});
#{$property}: calc(#{$expression});
}

/* slick slider css start */

.slick-slider{position:relative;display:block;box-sizing:border-box;-webkit-user-select:none;-moz-user-select:none;-ms-user-select:none;user-select:none;-webkit-touch-callout:none;-khtml-user-select:none;-ms-touch-action:pan-y;touch-action:pan-y;-webkit-tap-highlight-color:transparent;}
.slick-list{position:relative;display:block;overflow:hidden;margin:0;padding:0;}
.slick-list:focus{outline:none;}
.slick-list.dragging{cursor:pointer;cursor:hand;}
.slick-slider .slick-track,
.slick-slider .slick-list{-webkit-transform:translate3d(0, 0, 0);-moz-transform:translate3d(0, 0, 0);-ms-transform:translate3d(0, 0, 0);-o-transform:translate3d(0, 0, 0);transform:translate3d(0, 0, 0);}
.slick-track{position:relative;top:0;left:0;display:block;}
.slick-track:before,
.slick-track:after{display:table;content:'';}
.slick-track:after{clear:both;}
.slick-loading .slick-track{visibility:hidden;}
.slick-slide{display:none;float:left;height:100%;min-height:1px;}
[dir='rtl'] .slick-slide{float:right;}
.slick-slide img{display:block;}
.slick-slide.slick-loading img{display:none;}
.slick-slide.dragging img{pointer-events:none;}
.slick-initialized .slick-slide{display:block;}
.slick-loading .slick-slide{visibility:hidden;}
.slick-vertical .slick-slide{display:block;height:auto;border:1px solid transparent;}
.slick-arrow.slick-hidden{display:none;}

/* slick slider css end */



